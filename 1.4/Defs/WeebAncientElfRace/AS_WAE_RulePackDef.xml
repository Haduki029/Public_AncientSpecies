<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- キャラクター命名規則 -->
  <RulePackDef>
    <defName>AS_WAE_Name_Rule</defName>
    <include>
			<li>AS_NameLibrary</li>
		</include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[AS_Firstname] '[AS_Firstname]' [AS_Lastname]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

<!-- 村・街命名規則 -->
  <RulePackDef>
    <defName>AS_WAE_KingdomName_Rule</defName>
    <include>
			<li>AS_NameLibrary</li>
		</include>
    <rulePack>
      <rulesStrings>
        <!-- <li>name->エルフ神聖王国</li> -->
        <li>r_name->[AS_CountryNameNM]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
</Defs>
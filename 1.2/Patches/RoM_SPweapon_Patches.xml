<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>A RimWorld of Magic</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/TorannMagic.WeaponCategoryList[defName = "TM_Category_Pistols"]/weaponDefNames</xpath>
			<value>
				<li>AS_Handgun</li>
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>A RimWorld of Magic</li>
			<!--li>
				<packageId>Torann.ARimworldOfMagic</packageId>
			</li-->
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/TorannMagic.WeaponCategoryList[defName = "TM_Category_Rifles"]/weaponDefNames</xpath>
			<value>
				<li>AS_AssaultRifle</li>
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>A RimWorld of Magic</li>
			<!--li>
				<packageId>Torann.ARimworldOfMagic</packageId>
			</li-->
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/TorannMagic.WeaponCategoryList[defName = "TM_Category_Shotguns"]/weaponDefNames</xpath>
			<value>
				<li>AS_Shotgun</li>
				<li>AS_WHEAT_Shotgun</li>
			</value>
		</match>
	</Operation>
</Patch>

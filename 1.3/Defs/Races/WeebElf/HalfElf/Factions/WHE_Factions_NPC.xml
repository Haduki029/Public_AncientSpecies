<Defs>
  <FactionDef ParentName="WHE_NPC_FactionsBase">
    <defName>AS_WHE_Settlement</defName>
    <label>ハーフエルフの集落</label>
    <description>ハーフエルフ達の集落。</description>
    <colorSpectrum>
      <li>(0.45, 0.72, 0.27)</li>
    </colorSpectrum>
    <settlementTexturePath>WHE/WHE_Faction</settlementTexturePath>
    <factionIconPath>WHE/WHE_Faction</factionIconPath>
    <techLevel>Neolithic</techLevel>
    <settlementGenerationWeight>0.2</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxConfigurableAtWorldCreation>100</maxConfigurableAtWorldCreation>
		<configurationListOrderPriority>10</configurationListOrderPriority>
    <backstoryCategories>
      <li>AS_WHE_Villager</li>
    </backstoryCategories>
    <caravanTraderKinds>
			<li>AS_WHE_Caravan_Free_Marchant</li>
		</caravanTraderKinds>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,40)</li>
        <li>(70, 80)</li>
        <li>(700, 120)</li>
        <li>(1300, 240)</li>
        <li>(5000, 1200)</li>
        <li>(30000, 16000)</li>
        <li>(200000, 160000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <raidLootValueFromPointsCurve>
			<points>
			  <li>(35,     10)</li>
			  <li>(100,   40)</li>
			  <li>(1000,  400)</li>
			  <li>(2000, 900)</li>
			  <li>(4000, 1800)</li>
			</points>
		</raidLootValueFromPointsCurve>
    <allowedCultures MayRequire="Ludeon.RimWorld.Ideology">
			<li>AS_WHE_Cultures</li>
		</allowedCultures>
    <pawnGroupMakers>
      <li>
        <kindDef>Combat</kindDef>
        <commonality>100</commonality>
        <!-- <maxTotalPoints>5000</maxTotalPoints> -->
        <options>
          <AS_WHE_Warrior>3</AS_WHE_Warrior>
          <AS_WHE_Hunter>6</AS_WHE_Hunter>
          <AS_WHE_Villager>2</AS_WHE_Villager>
          <AS_WHE_EliteVillager>2</AS_WHE_EliteVillager>
        </options>
      </li>
      <li>
          <kindDef>Peaceful</kindDef>
          <options>
            <AS_WHE_Warrior>3</AS_WHE_Warrior>
            <AS_WHE_Hunter>5</AS_WHE_Hunter>
            <AS_WHE_EliteVillager>1</AS_WHE_EliteVillager>
            <AS_WHE_Villager>3</AS_WHE_Villager>
            <AS_WHE_Trader>1</AS_WHE_Trader>
          </options>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <AS_WHE_Warrior>2</AS_WHE_Warrior>
          <AS_WHE_Hunter>3</AS_WHE_Hunter>
          <AS_WHE_EliteVillager>1</AS_WHE_EliteVillager>
          <AS_WHE_Villager>3</AS_WHE_Villager>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
			  <AS_WHE_Trader>1</AS_WHE_Trader>
        </traders>
        <carriers>
          <Muffalo>4</Muffalo>
          <Dromedary>2</Dromedary>
          <Alpaca>3</Alpaca>
        </carriers>
        <guards>
          <AS_WHE_Villager>2</AS_WHE_Villager>
          <AS_WHE_Warrior>3</AS_WHE_Warrior>
          <AS_WHE_Hunter>5</AS_WHE_Hunter>
        </guards>
      </li>
    </pawnGroupMakers>
  </FactionDef>


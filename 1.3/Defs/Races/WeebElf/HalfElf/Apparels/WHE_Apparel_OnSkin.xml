<Defs>
  <ThingDef Class="AS_WHE.CustomThingDef" ParentName="AS_WHE_ApparelColorBase">
    <defName>AS_WHE_UnderwearA</defName>
    <description>皮や布で作られたノーマルな下着。</description>
    <label>下着（ハーフエルフ）</label>
    <tradeTags><li>AS_WHE_Apparel</li></tradeTags>
    <techLevel>Neolithic</techLevel>
    <graphicData>
      <texPath>WHE/Apparels/onskin/UnderWareA</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.2</drawSize>
    </graphicData>
    <followStuffColor>true</followStuffColor>
    <costStuffCount>40</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <WorkToMake>1200</WorkToMake>
      <Mass>0.1</Mass>
      <StuffEffectMultiplierArmor>0.05</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.55</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.32</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1</EquipDelay>
    </statBases>

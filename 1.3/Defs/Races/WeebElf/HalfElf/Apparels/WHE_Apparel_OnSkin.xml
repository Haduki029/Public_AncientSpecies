      <recipeUsers Inherit="False">
        <li>CraftingSpot</li>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
      <researchPrerequisite>AS_WHE_Apparel_Basic</researchPrerequisite>
      <skillRequirements>
        <Crafting>3</Crafting>
      </skillRequirements>
    </recipeMaker>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>WHE/Apparels/onskin/UnderWareA</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>AS_WHE_Colonist</li>
		    <li>AS_WHE_Villager</li>
        <li>AS_WHE_Neolithic</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>


<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- Check if Doragonian Race mod exists -->
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
	        <li>Dragonian Race</li>
			<li>1632800575</li>
			<li>GloomyLynx.DragonianRace</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName = "Dragonian_Female" or defName = "Dragonian_Male"]/alienRace/raceRestriction/weaponList</xpath>
			<value>
				<li>AS_AT_Impact_Knuckles</li>
			</value>
		</match>
	</Operation>
</Patch>